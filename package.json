{
  "name": "vue-chartless",
  "version": "0.1.22",
  "description": "100% Simplest reactive charts for the Vue.js",
  "author": "Nikola Kumric",
  "private": false,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rollup -c --environment BUILD:production",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "dist/vue-chartless.js"
  ],
  "browser": "./dist/vue-chartless.js",
  "unpkg": "./dist/vue-chartless.js",
  "keywords": [
    "vue",
    "chart",
    "vue-chart",
    "c3",
    "simplest",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumric/vue-chartless.git"
  },
  "dependencies": {
    "c3": "^0.7.1",
    "jquery": "^3.4.1",
    "vue": "^2.6.10",
    "vue-c3": "^1.2.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "rollup": "^1.13.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
